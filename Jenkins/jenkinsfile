pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'I am in Build Phase'
            }
        }
        
        stage('Test') {
            steps {
                echo 'I am in Test Phase'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'I am in Deploy Phase'
            }
        }
        stage('VersionCheck') {
            steps {
                echo 'I am in Version Phase'
                sh 'aws s3 ls'
            }
        }
        stage('Monitoring') {
            steps {
                echo 'Hello Monitoring Phase'
                build 'url-monitoring-job'
            }
        }
    }
}
